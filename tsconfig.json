{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [
      "es6",
      "dom",
      "es2016.array.include"
    ],
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es2018",
    "sourceMap": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "paths": {
      "metaes/*": [
        "node_modules/metaes/lib/*"
      ]
    }
  }
}